{
  "[python]": {
    "editor.defaultFormatter": null
  },
  "python.formatting.provider": "autopep8",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.linting.pylintArgs": [
    "--load-plugins",
    "pylint_django",
    "--django-settings-module=white_library.settings",
    "--disable=C0115,C0116" // disable missing-class-docstring, missing-function-docstring"
  ]
}
